	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_8
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_9
	yesnobox 0, 8
	compare VAR_RESULT, YES
	goto_if_eq PokemonShopCommon_EventScript_PokemonShopCheckPurchase_10
