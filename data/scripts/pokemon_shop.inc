	goto PokemonShopCommon_EventScript_PokemonShopEnd
	return

PokemonShopCommon_EventScript_PokemonShopCheckPurchase_3:
	playse SE_SUCCESS
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_1
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_2
	waitse
	goto PokemonShopCommon_EventScript_PokemonShopCheckPurchase_1

PokemonShopCommon_EventScript_PokemonShopCheckPurchase_4:
	playse SE_SUCCESS
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_3
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_4
	waitse
	goto PokemonShopCommon_EventScript_PokemonShopCheckPurchase_1

PokemonShopCommon_EventScript_PokemonShopCheckPurchase_5:
	playse SE_FAILURE
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_5
	waitse
	goto PokemonShopCommon_EventScript_PokemonShopEnd
	goto PokemonShopCommon_EventScript_PokemonShopCheckPurchase_1

PokemonShopCommon_EventScript_PokemonShopCheckPurchase_7:
	msgbox PokemonShopCommon_EventScript_PokemonShopCheckPurchase_Text_7
	removeitem ITEM_GOLD_BOTTLE_CAP, 1
	goto PokemonShopCommon_EventScript_PokemonShopCheckPurchase_6

PokemonShopCommon_EventScript_PokemonShopCheckPurchase_10:
	goto PokemonShopCommon_EventScript_PokemonShopCheckMoney
	goto PokemonShopCommon_EventScript_PokemonShopCheckPurchase_9


PokemonShopCommon_EventScript_PokemonShopEnd::
	msgbox PokemonShopCommon_EventScript_PokemonShopEnd_Text_0
	clearflag P_FLAG_FORCE_NO_SHINY
	clearflag P_FLAG_FORCE_SHINY
	hidemoneybox
	releaseall
	end


