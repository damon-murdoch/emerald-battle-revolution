script PokemonShopCommon_EventScript_PokemonShopCheckPurchase {
    msgbox(format("Sure! Let's see if I can get one of those for you ..."))
    switch(var(VAR_RESULT)){
        case MON_GIVEN_TO_PARTY:
            playse(SE_SUCCESS)
            msgbox(format("Here you go!"))
            msgbox(format("The POKéMON has been added to your party."))
            waitse
        case MON_GIVEN_TO_PC:
            playse(SE_SUCCESS)
            msgbox(format("All done!"))
            msgbox(format("The POKéMON has been sent to your PC boxes."))
            waitse
        case MON_CANT_GIVE:
            playse(SE_FAILURE)
            msgbox(format("It looks like I can't give you any more POKéMON right now - Sorry!"))
            waitse
            goto(PokemonShopCommon_EventScript_PokemonShopEnd)
    }
    msgbox(format("I'll take your payment now, please."))
    removemoney(4000)
    updatemoneybox
    playse(SE_SHOP)
    waitse
    if (flag(P_FLAG_FORCE_SHINY) == TRUE){
        msgbox(format("You also gave a Gold Bottle Cap in exchange."))
        removeitem(ITEM_GOLD_BOTTLE_CAP, 1)
    }
    msgbox(format("Thanks for your business!"))
    msgbox(format("Can we help you with anything else?"))
    yesnobox(0, 8)
    if (var(VAR_RESULT) == YES){
        goto(PokemonShopCommon_EventScript_PokemonShopCheckMoney)
    }
    goto(PokemonShopCommon_EventScript_PokemonShopEnd)
}

script PokemonShopCommon_EventScript_PokemonShopEnd {
    msgbox(format("Come back again soon!"))
    clearflag(P_FLAG_FORCE_NO_SHINY)
    clearflag(P_FLAG_FORCE_SHINY)
    hidemoneybox
    releaseall
    end
}

