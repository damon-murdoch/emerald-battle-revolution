script PokemonShopCommon_EventScript_PokemonShop {
    lock
    faceplayer
    msgbox(format("Hello there! I'm the POKéMON breeder! What are you looking for today?"))
    goto(PokemonShopCommon_EventScript_PokemonShopCheckMoney)
}

script PokemonShopCommon_EventScript_PokemonShopCheckMoney {
    showmoneybox(18,0)
    checkmoney(4000)
    if (var(VAR_RESULT) == TRUE){
        goto(PokemonShopCommon_EventScript_PokemonShopCheckGoldBottleCap)
    }
    else # Player does not have enough money
    {
        msgbox(format("Oh, sorry! You need at least ¥4000 to buy anything here!"))
        goto(PokemonShopCommon_EventScript_PokemonShopEnd)
    }
}

script PokemonShopCommon_EventScript_PokemonShopCheckGoldBottleCap {
    checkitem(ITEM_GOLD_BOTTLE_CAP)
    if (var(VAR_RESULT) == TRUE) {
        msgbox(format("Oh, you have a gold bottle cap! Would you redeem it for a shiny Pokemon?"))
        yesnobox(0, 8)
        if (var(VAR_RESULT) == TRUE) {
            clearflag(P_FLAG_FORCE_NO_SHINY)
            setflag(P_FLAG_FORCE_SHINY)
        }
        else {
            setflag(P_FLAG_FORCE_NO_SHINY)
            clearflag(P_FLAG_FORCE_SHINY)
        }
    }
    else {
        setflag(P_FLAG_FORCE_NO_SHINY)
        clearflag(P_FLAG_FORCE_SHINY)
    }

    msgbox(format("What are you looking for today?"))
    msgbox("L: Previous Page                    R: Next Page\nB: Go Back")
    goto(PokemonShopCommon_EventScript_PokemonShopRegions1)
}

script PokemonShopCommon_EventScript_PokemonShopRegions1 {
    multichoice2(0,0,PokemonShopCommon_Text_PokemonShopRegions1)
    switch(var(VAR_RESULT)){
        case 0: 
            goto(PokemonShopCommon_EventScript_PokemonShopKanto)
        case 1:
            goto(PokemonShopCommon_EventScript_PokemonShopJohto)
        case 2:
            goto(PokemonShopCommon_EventScript_PokemonShopHoenn)
        case 3:
            goto(PokemonShopCommon_EventScript_PokemonShopSinnoh)
        case 4:
            goto(PokemonShopCommon_EventScript_PokemonShopUnova)
        case 5:
            goto(PokemonShopCommon_EventScript_PokemonShopKalos)
        case MULTI_L_PRESSED: 
            goto(PokemonShopCommon_EventScript_PokemonShopRegions2)
        case MULTI_R_PRESSED:
            goto(PokemonShopCommon_EventScript_PokemonShopRegions2)
        default: 
            goto(PokemonShopCommon_EventScript_PokemonShopEnd)
    }
}

