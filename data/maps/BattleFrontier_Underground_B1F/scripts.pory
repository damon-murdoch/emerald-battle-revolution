## Exchange Machines ##

script BattleFrontier_Underground_B1F_EventScript_CurrencyConverter {
    msgbox(format("This is a machine for converting currency between BP and ¥!"))
    msgbox(format("The exchange rate is ¥9800 for every 1 BP."))
    goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom)
}

script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom {
    msgbox(format("What would you like to do?"))
    multichoice2(18,0,BattleFrontier_Underground_B1F_EventScript_CurrencyConverterBPToOrFrom)
    switch(var(VAR_RESULT)){
        case 0: // BP to ¥
            goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromOptions)
        case 1: // ¥ to BP
            goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOptions)
        default: // Cancel
            goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterEnd)
    }
}

script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOptions {
    showmoneybox(0,0)
    msgbox(format("How much would you like?"))
    specialvar(VAR_TEMP_1, GetFrontierBattlePoints)
    multichoice2(18,0,BattleFrontier_Underground_B1F_EventScript_CurrencyConverterBPToOptions)
    switch(var(VAR_RESULT)){
        case 0:
            checkmoney(9800)
            if (var(VAR_RESULT) == TRUE) {
                setvar(VAR_0x8004, 1)
                removemoney(9800)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford)
            }
        case 1:
            checkmoney(49000)
            if (var(VAR_RESULT) == TRUE) {
                setvar(VAR_0x8004, 5)
                removemoney(49000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford)
            }
        case 2:
            checkmoney(98000)
            if (var(VAR_RESULT) == TRUE) {
                setvar(VAR_0x8004, 10)
                removemoney(98000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford)
            }
        case 3:
            checkmoney(196000)
            if (var(VAR_RESULT) == TRUE) {
                setvar(VAR_0x8004, 20)
                removemoney(196000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford)
            }
        case 4:
            checkmoney(490000)
            if (var(VAR_RESULT) == TRUE) {
                setvar(VAR_0x8004, 50)
                removemoney(490000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford)
            }
        default:
            hidemoneybox
            goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom)
    }
    playse(SE_SHOP)
    special(GiveFrontierBattlePoints)
    updatemoneybox
    msgbox(format("BP redeemed successfully!"))
    waitse
    hidemoneybox
    goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOptions)
}
script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToCannotAfford {
    playse(SE_FAILURE)
    msgbox(format("Not enough ¥!"))
    waitse
    hidemoneybox
    goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom)
}
script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromOptions {
    special(ShowBattlePointsWindow)
    msgbox(format("How much would you like?"))
    specialvar(VAR_TEMP_1, GetFrontierBattlePoints)
    multichoice2(18,0,BattleFrontier_Underground_B1F_EventScript_CurrencyConverterBPFromOptions)
    switch(var(VAR_RESULT)){
        case 0:
            if (var(VAR_TEMP_1) >= 1) {
                setvar(VAR_0x8004, 1)
                addmoney(9800)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford)
            }
        case 1:
            if (var(VAR_TEMP_1) >= 5) {
                setvar(VAR_0x8004, 5)
                addmoney(49000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford)
            }
        case 2:
            if (var(VAR_TEMP_1) >= 10) {
                setvar(VAR_0x8004, 10)
                addmoney(98000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford)
            }
        case 3:
            if (var(VAR_TEMP_1) >= 20) {
                setvar(VAR_0x8004, 20)
                addmoney(196000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford)
            }
        case 4:
            if (var(VAR_TEMP_1) >= 50) {
                setvar(VAR_0x8004, 50)
                addmoney(490000)
            } else {
                goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford)
            }
        default:
            special(CloseBattlePointsWindow)
            goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom)
    }
    playse(SE_SHOP)
    special(TakeFrontierBattlePoints)
    special(UpdateBattlePointsWindow)
    msgbox(format("BP exchanged successfully!"))
    waitse
    special(CloseBattlePointsWindow)
    goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromOptions)
}
script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterFromCannotAfford {
    playse(SE_FAILURE)
    msgbox(format("Not enough BP!"))
    waitse
    special(CloseBattlePointsWindow)
    goto(BattleFrontier_Underground_B1F_EventScript_CurrencyConverterToOrFrom)
}

script BattleFrontier_Underground_B1F_EventScript_CurrencyConverterEnd {
    msgbox(format("You decided to stop using the converter."))
    release
    end
}

