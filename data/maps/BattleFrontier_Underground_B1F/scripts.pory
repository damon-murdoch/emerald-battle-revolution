script BattleFrontier_Underground_B1F_EventScript_PokemonShopMainClerkCheckPurchase {
    msgbox(format("Let's see if we can find one of those for you ..."))
    hidemoneybox
    delay(20)
    fadescreen(FADE_TO_BLACK)
    delay(20)
    fadescreen(FADE_FROM_BLACK)
    switch(var(VAR_RESULT)){
        case MON_GIVEN_TO_PARTY:
            playse(SE_SUCCESS)
            msgbox(format("Here you go!"))
            msgbox(format("The POKéMON has been added to your party."))
            waitse
        case MON_GIVEN_TO_PC:
            playse(SE_SUCCESS)
            msgbox(format("All done!"))
            msgbox(format("The POKéMON has been sent to your PC boxes."))
            waitse
        case MON_CANT_GIVE:
            playse(SE_FAILURE)
            msgbox(format("It looks like we can't give you any more POKéMON right now - Sorry!"))
            waitse
            goto(BattleFrontier_Underground_B1F_EventScript_PokemonShopMainClerkEnd)
    }
    msgbox(format("I'll take your payment now, please."))
    playse(SE_SHOP)
    removemoney(12000)
    showmoneybox(18,0)
    waitse
    msgbox(format("Thanks for your business!"))
    msgbox(format("Can we help you with anything else?"))
    yesnobox(0, 8)
    if (var(VAR_RESULT) == YES){
        goto(BattleFrontier_Underground_B1F_EventScript_PokemonShopMainClerkRegions1)
    }
    goto(BattleFrontier_Underground_B1F_EventScript_PokemonShopMainClerkEnd)
}
script BattleFrontier_Underground_B1F_EventScript_PokemonShopMainClerkEnd {
    msgbox(format("Come back again soon!"))
    hidemoneybox
    releaseall
    end
}
