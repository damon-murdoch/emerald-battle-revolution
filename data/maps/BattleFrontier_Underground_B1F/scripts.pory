# Game Corner Greeter
script BattleFrontier_Underground_B1F_EventScript_GameCornerGreeter {
    lock
	faceplayer
    msgbox(format("Welcome to the Battle Frontier Underground!"))
    msgbox(format("This is the entrance to the Battle Frontier Games Corner."))
    checkitem(ITEM_COIN_CASE)
    if (var(VAR_RESULT) == FALSE){
        msgbox(format("You will need a coin case to play here. Here you go!"))
        giveitem(ITEM_COIN_CASE)
    }
    msgbox(format("I hope you enjoy your stay!"))
    release
    end
}

